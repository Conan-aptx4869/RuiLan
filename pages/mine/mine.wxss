@import '../../common/common.wxss';

.login{
  width: 100%;
  color: #fff;
  font-size: 36rpx;
  height: 330rpx;
  padding: 0.1px;
  text-align: center;
  background: url(data:image/png;base64,)no-repeat center top;
  background-size: 100% 100%;
}
.loginBtn{
 background: none;
 color: #fff;
}
.avatar{
  width: 135rpx;
  height: 135rpx;
  background: #fff;
  border-radius:50%;
  display: block;
  margin: 25rpx auto 10rpx; 
}

.made{
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
}

.made{
  background-color: #fff;
}
.made .item{
  width: 250rpx;
  height: 160rpx;
  text-align: center;
  font-size: 24rpx;
  color: #333;
}
.made .item image{
  width: 42rpx;
  height: 50rpx;
  display: block;
  margin: 35rpx auto 17rpx;
}